{
  "name": "mr-next-starter",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --noemit",
    "lint-js": "eslint --fix --ignore-path .eslintignore --ignore-pattern \"!**/.*\" ."
  },
  "dependencies": {
    "@types/react-redux": "^7.1.18",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.21.1",
    "js-cookie": "^3.0.1",
    "next": "^12.1.1",
    "next-redux-wrapper": "^7.0.5",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.3",
    "react-redux-loading-bar": "^5.0.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/styled-components": "^5.1.11",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^3.0.0",
    "jest": "~25.2.6",
    "lint-staged": "^7.3.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "~16.13.1",
    "redux-devtools-extension": "^2.13.9",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add --force"
    ],
    "*.{md,graphql}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint --fix",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
